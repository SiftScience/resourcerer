{
  "presets": [
    ["@babel/env", {
      "targets": {
        "chrome": "80",
        "firefox": "79",
        "edge": "84",
        "safari": "13.1"
      }
    }],
    "@babel/react"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["@babel/proposal-decorators", {"legacy": true}]
  ],
  "ignore": ["**/node_modules/**"]
}
